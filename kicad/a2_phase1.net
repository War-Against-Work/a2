  <!-- ========================================================= -->
  <!-- Nets                                                      -->
  <!-- ========================================================= -->
  <nets>
    <!-- Power Rails -->
    <net code="0" name="GND">
      <node ref="PSU1" pin="-V"/>
      <node ref="MOSFET1" pin="VIN-"/>
      <node ref="MOSFET1" pin="OUT-"/>
      <node ref="BUCK5V1" pin="GND"/>
      <node ref="BUCK3V3_1" pin="GND"/>
      <node ref="PI1" pin="GND"/>
      <node ref="TEENSY1" pin="GND"/>
      <node ref="MUX1" pin="GND"/>
      <!-- Additional GND nodes omitted for brevity -->
    </net>

    <net code="1" name="+12V">
      <node ref="PSU1" pin="+V"/>
      <node ref="MOSFET1" pin="VIN+"/>
      <node ref="MOSFET1" pin="OUT+"/>
      <node ref="DRV1" pin="VM"/>
      <node ref="DRV2" pin="VM"/>
      <node ref="DRV3" pin="VM"/>
      <node ref="DRV4" pin="VM"/>
      <node ref="DRV5" pin="VM"/>
      <node ref="DRV6" pin="VM"/>
      <node ref="CAP_BULK1" pin="1"/>
    </net>

    <net code="2" name="+5V">
      <node ref="BUCK5V1" pin="VOUT"/>
      <node ref="PI1" pin="5V"/>
      <node ref="U2D2_1" pin="VDD"/>
      <!-- Sensors powered via USB / 5V rail can be added here -->
    </net>

    <net code="3" name="+3V3">
      <node ref="BUCK3V3_1" pin="VOUT"/>
      <node ref="TEENSY1" pin="3V3"/>
      <node ref="MUX1" pin="VCC"/>
      <node ref="IMU_HEAD" pin="VCC"/>
      <node ref="IMU_BASE" pin="VCC"/>
      <node ref="TOF_NEAR" pin="VCC"/>
      <node ref="VLX_FRONT" pin="VCC"/>
      <node ref="VLX_BACK" pin="VCC"/>
      <node ref="VLX_LEFT" pin="VCC"/>
      <node ref="VLX_RIGHT" pin="VCC"/>
      <node ref="INA1" pin="VCC"/>
      <node ref="INA2" pin="VCC"/>
      <node ref="INA3" pin="VCC"/>
      <node ref="INA4" pin="VCC"/>
      <node ref="INA5" pin="VCC"/>
      <node ref="INA6" pin="VCC"/>
    </net>

    <!-- I2C Main Bus (Pi ↔ Mux) -->
    <net code="4" name="I2C_SDA">
      <node ref="PI1" pin="SDA1"/>
      <node ref="MUX1" pin="SDA"/>
    </net>
    <net code="5" name="I2C_SCL">
      <node ref="PI1" pin="SCL1"/>
      <node ref="MUX1" pin="SCL"/>
    </net>

    <!-- UART between Pi & Teensy -->
    <net code="6" name="UART0_TX_PI">
      <node ref="PI1" pin="TXD0"/>
      <node ref="TEENSY1" pin="RX1"/>
    </net>
    <net code="7" name="UART0_RX_PI">
      <node ref="PI1" pin="RXD0"/>
      <node ref="TEENSY1" pin="TX1"/>
    </net>

    <!-- Soft-Start Control -->
    <net code="8" name="SOFTSTART_CTRL">
      <node ref="PI1" pin="GPIO26"/>
      <node ref="MOSFET1" pin="CTRL"/>
    </net>

    <!-- Dynamixel Bus (half-duplex) -->
    <net code="9" name="DXL_BUS">
      <node ref="U2D2_1" pin="DATA"/>
      <node ref="SERVO_YAW" pin="DATA"/>
      <node ref="SERVO_PITCH" pin="DATA"/>
      <node ref="SERVO_ROLL" pin="DATA"/>
    </net>

    <!-- Example Actuator Driver Connections (A1) -->
    <net code="10" name="A1_PWM">
      <node ref="TEENSY1" pin="PWM2"/>
      <node ref="DRV1" pin="IN1"/>
    </net>
    <net code="11" name="A1_MOTOR+">
      <node ref="DRV1" pin="OUT1"/>
      <node ref="ACT1" pin="M+"/>
    </net>
    <net code="12" name="A1_MOTOR-">
      <node ref="DRV1" pin="OUT2"/>
      <node ref="ACT1" pin="M-"/>
    </net>
    <!-- Additional actuator nets (A2-A6) analogous, omitted for brevity -->
  </nets>
</export> 