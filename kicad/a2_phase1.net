    <comp ref="ACT5">
      <value>Actuonix_L16</value>
      <footprint>Connector:Actuator_Pads</footprint>
    </comp>
    <comp ref="ACT6">
      <value>Actuonix_L16</value>
      <footprint>Connector:Actuator_Pads</footprint>
    </comp>

    <!-- Dynamixel Chain -->
    <comp ref="U2D2_1">
      <value>Robotis_U2D2</value>
      <footprint>Connector:U2D2</footprint>
    </comp>
    <comp ref="SERVO_YAW">
      <value>Dynamixel_XL430</value>
      <footprint>Motor:Dynamixel_XL430</footprint>
    </comp>
    <comp ref="SERVO_PITCH">
      <value>Dynamixel_XL430</value>
      <footprint>Motor:Dynamixel_XL430</footprint>
