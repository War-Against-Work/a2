<?xml version="1.0" encoding="utf-8"?>
<export version="D">
  <design>
    <source>docs/hardware/wiring-guide.md</source>
    <date>2025-06-14</date>
    <tool>A2-Robot Wiring-Guide → KiCad Netlist (generated by AI assistant)</tool>
  </design>

  <!-- ========================================================= -->
  <!-- Components                                                -->
  <!-- ========================================================= -->
  <components>
    <!-- Power & Power-Control -->
    <comp ref="PSU1">
      <value>MEANWELL_LRS-350-12</value>
      <footprint>PowerSupply:MeanWell_LRS-350-12</footprint>
    </comp>
    <comp ref="MOSFET1">
      <value>SoftStart_Anmbest_MOSFET_Module</value>
      <footprint>Module:Anmbest_MOSFET_Switch</footprint>
    </comp>
    <comp ref="BUCK5V1">
      <value>DC-DC_Buck_5V_5A</value>
      <footprint>Converter_DC-DC:MP1584_Module</footprint>
    </comp>
    <comp ref="BUCK3V3_1">
      <value>DC-DC_Buck_3V3_2A</value>
      <footprint>Converter_DC-DC:MP1584_Module</footprint>
    </comp>
    <comp ref="CAP_BULK1">
      <value>10000uF_25V</value>
      <footprint>Capacitor_THT:CP_Radial_D22.0mm_P10.00mm</footprint>
    </comp>

    <!-- Controllers -->
    <comp ref="PI1">
      <value>Raspberry_Pi_5</value>
      <footprint>Module:Raspberry_Pi_5</footprint>
    </comp>
    <comp ref="TEENSY1">
      <value>Teensy_4.1</value>
      <footprint>Module:Teensy4.1</footprint>
    </comp>

    <!-- I2C Multiplexer & Sensors -->
    <comp ref="MUX1">
      <value>TCA9548A</value>
      <footprint>Package_SO:SOIC-24_7.5x15.4mm_P1.27mm</footprint>
    </comp>
    <comp ref="IMU_HEAD">
      <value>ICM-20948</value>
      <footprint>Module:SparkFun_ICM-20948_Breakout</footprint>
    </comp>
    <comp ref="IMU_BASE">
      <value>ICM-20948</value>
      <footprint>Module:SparkFun_ICM-20948_Breakout</footprint>
    </comp>
    <comp ref="TOF_NEAR">
      <value>Arducam_ToF</value>
      <footprint>Module:Arducam_ToF_Board</footprint>
    </comp>
    <comp ref="VLX_FRONT">
      <value>VL53L0X</value>
      <footprint>Module:VL53L0X_Breakout</footprint>
    </comp>
    <comp ref="VLX_BACK">
      <value>VL53L0X</value>
      <footprint>Module:VL53L0X_Breakout</footprint>
    </comp>
    <comp ref="VLX_LEFT">
      <value>VL53L0X</value>
      <footprint>Module:VL53L0X_Breakout</footprint>
    </comp>
    <comp ref="VLX_RIGHT">
      <value>VL53L0X</value>
      <footprint>Module:VL53L0X_Breakout</footprint>
    </comp>
    <comp ref="INA1">
      <value>INA219</value>
      <footprint>Module:INA219_Breakout</footprint>
    </comp>
    <comp ref="INA2">
      <value>INA219</value>
      <footprint>Module:INA219_Breakout</footprint>
    </comp>
    <comp ref="INA3">
      <value>INA219</value>
      <footprint>Module:INA219_Breakout</footprint>
    </comp>
    <comp ref="INA4">
      <value>INA219</value>
      <footprint>Module:INA219_Breakout</footprint>
    </comp>
    <comp ref="INA5">
      <value>INA219</value>
      <footprint>Module:INA219_Breakout</footprint>
    </comp>
    <comp ref="INA6">
      <value>INA219</value>
      <footprint>Module:INA219_Breakout</footprint>
    </comp>

    <!-- Actuator Drivers & Actuators -->
    <comp ref="DRV1">
      <value>DRV8871</value>
      <footprint>Module:DRV8871_Breakout</footprint>
    </comp>
    <comp ref="DRV2">
      <value>DRV8871</value>
      <footprint>Module:DRV8871_Breakout</footprint>
    </comp>
    <comp ref="DRV3">
      <value>DRV8871</value>
      <footprint>Module:DRV8871_Breakout</footprint>
    </comp>
    <comp ref="DRV4">
      <value>DRV8871</value>
      <footprint>Module:DRV8871_Breakout</footprint>
    </comp>
    <comp ref="DRV5">
      <value>DRV8871</value>
      <footprint>Module:DRV8871_Breakout</footprint>
    </comp>
    <comp ref="DRV6">
      <value>DRV8871</value>
      <footprint>Module:DRV8871_Breakout</footprint>
    </comp>
    <comp ref="ACT1">
      <value>Actuonix_L16</value>
      <footprint>Connector:Actuator_Pads</footprint>
    </comp>
    <comp ref="ACT2">
      <value>Actuonix_L16</value>
      <footprint>Connector:Actuator_Pads</footprint>
    </comp>
    <comp ref="ACT3">
      <value>Actuonix_L16</value>
      <footprint>Connector:Actuator_Pads</footprint>
